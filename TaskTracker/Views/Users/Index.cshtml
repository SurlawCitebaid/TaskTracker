@model IEnumerable<TaskTracker.Models.User>

<h2>Users</h2>
<a href="@Url.Action("Create", "Users")">Create New User</a>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Name</td>
            <td>@user.Email</td>
        </tr>
    }
    </tbody>
</table>